{
    "name": "portabilis/i-educar",
    "description": "Software livre de gest√£o escolar",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "version": "2.6.0",
    "keywords": [
        "Portabilis",
        "i-Educar"
    ],
    "homepage": "https://github.com/portabilis/i-educar",
    "require": {
        "php": "^7.4",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "aws/aws-sdk-php-laravel": "^3.4",
        "composer/semver": "^1.4",
        "cossou/jasperphp": "^2.7",
        "dex/composer-plug-and-play": "~0.1",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "honeybadger-io/honeybadger-laravel": "^3.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "league/csv": "^9.6",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mll-lab/graphql-php-scalars": "^3.1",
        "nuwave/lighthouse": "^4.18",
        "phpmailer/phpmailer": "^6.3",
        "phpoffice/phpspreadsheet": "^1.6",
        "predis/predis": "^1.1",
        "swiftmailer/swiftmailer": "^6.1",
        "tooleks/laravel-asset-version": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "laravel/sail": "^0.0.5",
        "laravel/telescope": "^4.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "brianium/paratest": "^6.2"
    },
    "autoload": {
        "classmap": [
            "database/factories",
            "ieducar/intranet/include",
            "ieducar/lib",
            "ieducar/modules/*/Model",
            "ieducar/modules/Avaliacao/Service",
            "ieducar/modules/Avaliacao/Fixups",
            "ieducar/modules/Avaliacao/Views",
            "ieducar/modules/FormulaMedia/Validate"
        ],
        "files": [
            "ieducar/intranet/include/funcoes.inc.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "iEducar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/Unit"
        ]
    },
    "include-path": [
        "ieducar",
        "ieducar/intranet",
        "ieducar/lib",
        "ieducar/modules"
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "check-style": "vendor/bin/php-cs-fixer --diff --dry-run --verbose fix",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "set-permissions": "find bootstrap/cache -type d -exec chmod 777 {} + && find storage -type d -exec chmod 777 {} +",
        "npm": "npm install",
        "new-install": [
            "@composer install",
            "@set-permissions",
            "@php artisan key:generate --ansi",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --seed --force --ansi",
            "@npm"
        ],
        "update-install": [
            "@composer update --plug-and-play",
            "@set-permissions",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --force --ansi",
            "@npm"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
      ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
