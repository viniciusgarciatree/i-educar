{
    "name": "portabilis/i-educar",
    "description": "Software livre de gest√£o escolar",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "version": "2.2.20",
    "keywords": [
        "Portabilis",
        "i-Educar"
    ],
    "homepage": "https://github.com/portabilis/i-educar",
    "require": {
        "php": "^7.2",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "aws/aws-sdk-php-laravel": "^3.4",
        "barryvdh/laravel-cors": "^0.11.4",
        "cocur/slugify": "^3.1",
        "composer/semver": "^1.4",
        "cossou/jasperphp": "^2.7",
        "doctrine/dbal": "^2.9",
        "edersoares/laravel-plug-and-play": "^1.0",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "honeybadger-io/honeybadger-laravel": "^1.4",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.4",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "swiftmailer/swiftmailer": "^6.1",
        "tooleks/laravel-asset-version": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.1",
        "laravel/telescope": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "ieducar/intranet/include",
            "ieducar/lib"
        ],
        "files": [
            "ieducar/intranet/include/funcoes.inc.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "iEducar\\": "src",
            "iEducar\\Packages\\Reports\\": "ieducar/modules/Reports/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/Unit"
        ]
    },
    "include-path": [
        "ieducar",
        "ieducar/intranet",
        "ieducar/lib",
        "ieducar/modules"
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "laravel/telescope",
                "barryvdh/laravel-debugbar"
            ]
        },
        "merge-plugin": {
            "include": [
                "packages/*/*/composer.json"
            ]
        }
    },
    "scripts": {
        "check-style": "vendor/bin/php-cs-fixer --diff --dry-run --verbose fix",
        "fix-style": "vendor/bin/php-cs-fixer fix",
        "set-permissions": "find bootstrap/cache -type d -exec chmod 777 {} + && find storage -type d -exec chmod 777 {} +",
        "new-install": [
            "@composer install",
            "@set-permissions",
            "@php artisan key:generate --ansi",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --seed --force --ansi"
        ],
        "update-install": [
            "@composer install",
            "@set-permissions",
            "@php artisan legacy:link --ansi",
            "@php artisan migrate --force --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
